{
  "name": "@akashic/akashic-cli-init",
  "version": "0.3.5",
  "description": "A module to get your Akashic game started.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "gulp clean && npm run install:typings && gulp compile && gulp zip",
    "install:typings": "gulp install:typings",
    "clean:typings": "gulp clean:typings",
    "clean": "gulp clean",
    "start": "npm run build && node bin/run",
    "build": "gulp compile && gulp zip",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "pretest": "npm run build",
    "test": "gulp test lint-md && npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-cli-init": "./bin/run"
  },
  "files": [
    "README.md",
    "bin",
    "lib",
    "templates",
    "package.json"
  ],
  "devDependencies": {
    "del": "~3.0.0",
    "gulp": "~4.0.0",
    "gulp-istanbul": "~1.1.3",
    "gulp-jasmine": "~2.3.0",
    "gulp-shell": "~0.5.2",
    "gulp-tslint": "~4.3.5",
    "gulp-zip": "^4.0.0",
    "http-server": "~0.11.1",
    "jasmine": "~3.1.0",
    "jasmine-reporters": "~2.0.4",
    "jasmine-terminal-reporter": "~0.9.1",
    "mdast": "~2.3.2",
    "mdast-lint": "~1.1.1",
    "mock-fs": "~4.7.0",
    "tslint": "^5.4.3",
    "typescript": "~2.1.6",
    "typings": "2.1.1"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-cli-config": "~0.1.0",
    "@akashic/akashic-cli-commons": "~0.2.0",
    "commander": "2.8.1",
    "fs-extra": "0.30.0",
    "prompt": "1.0.0",
    "unzip": "^0.1.11",
    "request": "^2.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-cli-init.git"
  },
  "publishConfig": {
    "@akashic:registry": "https://registry.npmjs.org/"
  }
}
